{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "PhyClone Cluster Input Schema",
  "description": "PhyClone cluster data .tsv file validation schema",
  "type": "object",
  "properties": {
    "mutation_id": {
      "type": [
        "string",
        "integer"
      ],
      "minLength": 1
    },
    "sample_id": {
      "type": [
        "string",
        "integer"
      ],
      "minLength": 1
    },
    "cluster_id": {
      "type": [
        "string",
        "integer"
      ],
      "minLength": 1
    },
    "cellular_prevalence": {
      "type": "number",
      "minimum": 0.0
    },
    "outlier_prob": {
      "type": "number",
      "minimum": 0.0
    },
    "chrom": {
      "type": [
        "string",
        "integer"
      ],
      "minLength": 1
    }
  },
  "required": [
    "mutation_id",
    "cluster_id"
  ]
}